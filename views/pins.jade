extends layout

block content
  h1 #{title}
  
  tr
    td
      a(href="./home?id="+session_userid) Home <br/>
      <br/>

  table(border='1')
    - for (var inx = 0; inx < results.length; inx++)
        tr
          td
            a(href="/pins/pinsphoto?id=" + queried_userid + "&pid=" + results[inx].PHOTOID) #{results[inx].PHOTOID}
          td 
            img(src=results[inx].URL, height="240" width="320") 
  